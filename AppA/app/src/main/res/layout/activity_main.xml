<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.pythoncat.appa.MainActivity">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hello World!" />

    <Button
        android:id="@+id/btn_call_b_engine"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/call_app_b" />

    <Button
        android:id="@+id/btn_call_1024"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/b_int" />

    <Button
        android:id="@+id/btn_call_1024_async"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/b_int_async" />
    <Button
        android:id="@+id/btn_call_net_pn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/b" />
</LinearLayout>
